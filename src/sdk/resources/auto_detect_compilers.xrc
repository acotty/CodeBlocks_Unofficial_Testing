<?xml version="1.0" encoding="utf-8" ?>
<resource xmlns="http://www.wxwidgets.org/wxxrc" version="2.5.3.0">
	<object class="wxScrollingDialog" name="dlgAutoDetectCompilers">
		<title>Compilers auto-detection</title>
		<centered>1</centered>
		<size>900,407</size>
		<style>wxCAPTION|wxRESIZE_BORDER|wxMAXIMIZE_BOX</style>
		<object class="wxBoxSizer">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<object class="wxStaticText" name="ID_STATICTEXT2">
					<label>Note: After auto-detection, either the default compiler was not detected or at least one compiler&apos;s master path is still empty and therefore invalid.&#x0A;Inspect the list below and select another default compiler if it was not found or and change the compiler&apos;s master path later in the compiler options if the compiler is invalid.&#x0A;Select you favourite default compiler here:</label>
					<size>907,59</size>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>8</border>
			</object>
			<object class="sizeritem">
				<object class="wxFlexGridSizer">
					<cols>2</cols>
					<vgap>8</vgap>
					<hgap>8</hgap>
					<growablecols>0</growablecols>
					<growablerows>0</growablerows>
					<object class="sizeritem">
						<object class="wxListCtrl" name="lcCompilers">
							<size>523,200</size>
							<style>wxLC_REPORT|wxLC_SINGLE_SEL</style>
						</object>
						<flag>wxEXPAND|wxFIXED_MINSIZE</flag>
					</object>
					<object class="sizeritem">
						<object class="wxGridSizer">
							<cols>1</cols>
							<rows>3</rows>
							<vgap>10</vgap>
							<object class="sizeritem">
								<object class="wxButton" name="btnSetDefaultCompiler">
									<label>Set as default</label>
									<pos>-1,-1</pos>
									<size>-1,-1</size>
								</object>
								<flag>wxALIGN_TOP|wxALIGN_CENTER_HORIZONTAL</flag>
								<border>5</border>
							</object>
							<object class="sizeritem">
								<object class="wxRadioBox" name="rbCompilerShowOptions">
									<label>Show</label>
									<content>
										<item>All</item>
										<item>Detected</item>
									</content>
									<default>1</default>
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL</flag>
								<border>5</border>
							</object>
							<object class="sizeritem">
								<object class="wxCheckBox" name="cbShowCompilerPath">
									<label>Show Path</label>
								</object>
								<flag>wxALL|wxALIGN_LEFT</flag>
								<border>5</border>
							</object>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxBOTTOM|wxLEFT|wxRIGHT|wxEXPAND</flag>
				<border>8</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxBoxSizer">
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT1">
							<label>Current default compiler:</label>
						</object>
						<flag>wxALIGN_TOP</flag>
					</object>
					<object class="sizeritem">
						<object class="wxStaticText" name="lblDefCompiler">
							<label>xxx</label>
							<fg>#800000</fg>
						</object>
						<flag>wxLEFT|wxRIGHT|wxEXPAND</flag>
						<border>8</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxBOTTOM|wxLEFT|wxRIGHT|wxEXPAND</flag>
				<border>8</border>
			</object>
			<object class="sizeritem">
				<object class="wxStaticLine" name="ID_STATICLINE1" />
				<flag>wxLEFT|wxRIGHT|wxEXPAND</flag>
				<border>8</border>
			</object>
			<object class="sizeritem">
				<object class="wxBoxSizer">
					<object class="sizeritem">
						<object class="wxButton" name="wxID_OK">
							<size>-1,-1</size>
				</object>
				<flag>wxALL|wxEXPAND</flag>
					</object>
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL</flag>
				<border>8</border>
			</object>
		</object>
	</object>
</resource>
