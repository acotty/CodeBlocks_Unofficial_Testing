<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="AutoVersioning MacOS" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/codeblocks/plugins/AutoVersioning" prefix_auto="1" extension_auto="1" />
				<Option object_output="$(#CB_BUILD)/.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/contrib/AutoVersioning" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-fPIC" />
					<Add option="$(#CB_BUILD.CFLAGS)" />
					<Add option="`$(#WXWIDGETS.WX_CONFIG_MACOS) --cflags`" />
					<Add option="-DPIC" />
					<Add directory="$(#CB_BUILD.INCLUDE)" />
					<Add directory="$(#CB_BUILD.INCLUDE)/tinyxml" />
				</Compiler>
				<Linker>
					<Add option="`$(#WXWIDGETS.WX_CONFIG_MACOS) --libs`" />
					<!-- Add option="-Wl,--no-undefined" -->
					<Add library="codeblocks" />
					<Add directory="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 $(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/codeblocks/AutoVersioning.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-D__WXMAC__" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
		</Compiler>
		<Unit filename="AutoVersioning.cpp" />
		<Unit filename="AutoVersioning.h" />
		<Unit filename="avChangesDlg.cpp" />
		<Unit filename="avChangesDlg.h" />
		<Unit filename="avConfig.h" />
		<Unit filename="avHeader.cpp" />
		<Unit filename="avHeader.h" />
		<Unit filename="avSvnRevision.cpp" />
		<Unit filename="avSvnRevision.h" />
		<Unit filename="avVersionEditorDlg.cpp" />
		<Unit filename="avVersionEditorDlg.h" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<wxsmith version="1">
				<gui name="wxWidgets" src="avVersionEditorDlg.cpp" main="avVersionEditorDlg" init_handlers="necessary" language="CPP" />
				<resources>
					<wxScrollingDialog wxs="wxsmith/avVersionEditorDlg.wxs" src="avVersionEditorDlg.cpp" hdr="avVersionEditorDlg.h" fwddecl="0" i18n="1" name="avVersionEditorDlg" language="CPP" />
					<wxScrollingDialog wxs="wxsmith/avChangesDlg.wxs" src="avChangesDlg.cpp" hdr="avChangesDlg.h" fwddecl="0" i18n="1" name="avChangesDlg" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
