<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="cbBuildTools" />
		<Option pch_mode="2" />
		<Option default_target="cbBuildTools_to_codeblocks" />
		<Option compiler="gcc" />
		<Build>
			<Target title="cbBuildTools_create_plugin">
				<Option output="bin$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/cbBuildTools" prefix_auto="1" extension_auto="1" />
				<Option working_dir="bin$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				<Option object_output=".objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<ExtraCommands>
					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />

					<Add after="cmd /c if exist cbBuildTools.zip del cbBuildTools.zip" />
					<!Add after="cmd /c if exist cbBuildTools.cbplugin del cbBuildTools.cbplugin" />

					<Add after="cmd /c copy $(TARGET_OUTPUT_FILE)" />
					<Add after="strip cbBuildTools.dll" />
					<Add after="zip -j9 cbBuildTools.zip manifest.xml" />
					<Add after="zip -9R cbBuildTools.zip images\*" />
					<Add after="zip -j9 cbBuildTools.zip res\*" />
					<Add after="zip -j9 cbBuildTools.cbplugin cbBuildTools.dll cbBuildTools.zip" />
					<Add after="cmd /c if exist cbBuildTools.zip del cbBuildTools.zip" />
					<Add after="cmd /c del cbBuildTools.dll" />

					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="cbBuildTools_to_codeblocks">
				<Option output="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/cbBuildTools" prefix_auto="1" extension_auto="1" />
				<Option working_dir="bin$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				<Option object_output=".objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance --no-splash-screen --verbose -p debug --no-dde --no-check-associations " />
				<Option host_application="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<ExtraCommands>
					<Add before="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />

					<Add after="cmd /c if exist $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\cbBuildTools.zip del $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\cbBuildTools.zip" />

					<Add after="zip -R9  $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\cbBuildTools.zip images\*" />
					<Add after="zip -j9 $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\cbBuildTools.zip res\*" />
					<Add after="zip -j9 $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\cbBuildTools.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="cbBuildTools_to_codeblocks;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="$(#CB_BUILD.CFLAGS)" />
			<Add option="-pipe" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-mthreads" />
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add option="-m$(#CB_BUILD.OSBITS)" />
			<Add option="-fno-keep-inline-dllexport" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />
			<Add directory="$(#CB_BUILD)/include" />
			<Add directory="$(#CB_BUILD)/sdk/wxscintilla/include" />
			<Add directory="$(#CB_BUILD)/include/tinyxml" />
			<Add directory="include" />
		</Compiler>
		<Linker>
			<Add option="-Wl,--enable-auto-import" />
			<Add option="-Wl,--no-undefined" />
			<Add option="-mthreads" />
			<Add directory="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
			<Add library="codeblocks" />
			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)" />
			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
		</Linker>
		<Unit filename="cbBuildTools.cpp" />
		<Unit filename="cbBuildTools.h" />
		<Unit filename="include/buildtargetswindow.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="res/dummy.xrc" />
		<Unit filename="src/buildtargetswindow.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
