<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Help Windows" />
		<Option pch_mode="0" />
		<Option default_target="help_plugin" />
		<Option compiler="gcc" />
		<Build>
			<Target title="bzip2">
				<Option output="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/libbz2_help.a" prefix_auto="0" extension_auto="0" />
				<Option working_dir="" />
				<Option object_output="$(#CB_BUILD)/.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/contrib/help_plugin/bzip2" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-mthreads" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
				</Linker>
			</Target>
			<Target title="zlib">
				<Option output="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/libz.a" prefix_auto="0" extension_auto="0" />
				<Option working_dir="" />
				<Option object_output="$(#CB_BUILD)/.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/contrib/help_plugin/zlib" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-mthreads" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
				</Linker>
			</Target>
			<Target title="help_plugin">
				<Option output="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/share/CodeBlocks/plugins/help_plugin.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="$(#CB_BUILD)/.objs$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/plugins/contrib/help_plugin" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="$(#CB_BUILD.CFLAGS)" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-Winvalid-pch" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-D_WIN$(#CB_BUILD.OSBITS)" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="bzip2" />
					<Add directory="zlib" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
					<Add library="bz2_help" />
					<Add library="z" />
				</Linker>
				<ExtraCommands>
					<!-- Create output directories -->
					<Add before="cmd /c &quot;if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)&quot;" />
					<Add before="cmd /c &quot;if not exist $(#CB_BUILD)\output$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\docs\ mkdir $(#CB_BUILD)\output$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\docs\&quot;" />
					<Mode before="always" />

					<!-- Create help_plugin.zip -->
					<Add after="zip -j9  $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\help_plugin.zip manifest.xml *.xrc" />
					<Add after="zip -rq9 $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\help_plugin.zip .\images" />

					<!-- Copy file to docs directory -->
					<Add before="cmd /c &quot;if not exist $(#CB_BUILD)\..\windows_installer xcopy /Y index.ini $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\docs\&quot;" />
					<Add before="cmd /c &quot;if exist $(#CB_BUILD)\..\windows_installer xcopy /Y $(#CB_BUILD)\..\windows_installer\Documentation\*.* $(#CB_BUILD)\devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)\share\CodeBlocks\docs\&quot;" />

					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="bzip2;zlib;help_plugin;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-std=$(#CB_BUILD.CPP_STD)" />
			<Add directory="$(#WXWIDGETS.include)" />
			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)/msw$(#WXWIDGETS.WX_SUFFIX)" />
			<Add directory="$(#CB_BUILD.INCLUDE)" />
			<Add directory="$(#CB_BUILD.INCLUDE)/tinyxml" />
			<Add directory="$(#CB_BUILD)/sdk/wxscintilla/include" />
			<Add directory="$(#CB_BUILD.INCLUDE)/scripting/include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WXWIDGETS.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-Wl,--enable-auto-image-base" />
			<Add option="-Wl,--add-stdcall-alias" />
			<Add option="-Wl,--enable-auto-import" />
			<Add library="wxmsw$(#WXWIDGETS.WX_VERSION)$(#WXWIDGETS.WX_SUFFIX)" />
			<Add library="codeblocks" />
			<Add directory="$(#CB_BUILD)/devel$(#WXWIDGETS.WX_VERSION)_$(#CB_BUILD.OSBITS)" />
			<Add directory="$(#WXWIDGETS.lib)/gcc_dll$(#WXWIDGETS.WX_CFG)" />
		</Linker>
		<Unit filename="HelpConfigDialog.cpp">
			<Option target="help_plugin" />
		</Unit>
		<Unit filename="HelpConfigDialog.h">
			<Option target="help_plugin" />
		</Unit>
